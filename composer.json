{
  "name": "sbamtr/laravel-source-encrypter",
  "keywords": [
    "laravel",
    "lumen",
    "source",
    "encrypt",
    "encryption",
    "encrypter",
    "encryptor",
    "phpBolt",
    "ionCube"
  ],
  "type": "library",
  "require": {
    "php": ">=7.2.5",

    "illuminate/console": "^6|^7|^8|^9|^10|^11|^12",
    "ext-bolt": "*"
  },
  "description": "Laravel Source Encrypter",
  "license": "MIT",
  "authors": [
    {
      "name": "Siavash Bamshadnia",
      "email": "sbamtr@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "sbamtr\\LaravelSourceEncrypter\\": "src"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "sbamtr\\LaravelSourceEncrypter\\SourceEncryptServiceProvider"
      ]
    }
  }
}
  
